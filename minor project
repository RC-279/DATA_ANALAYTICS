SELECT CUSTOMER.CUST_CODE ,ORDERS.ORD_NUM, ORDERS.ORD_DATE ,CUSTOMER.CUST_NAME , CUSTOMER.receive_amt , ORDERS.ORD_DESCRIPTION FROM orders INNER JOIN customer On CUSTOMER.CUST_CODE = ORDERS.CUST_CODE ;
SELECT CUSTOMER.CUST_CODE ,CUSTOMER.CUST_NAME , CUSTOMER.opening_amt , CUSTOMER.RECEIVE_AMT , CUSTOMER.PAYMENT_AMT,CUSTOMER.OUTSTANDING_AMT,ORDERS.advance_amount AS ANY_ADV_AMT_GIVEN_ON_ANY_ORDERS FROM orders INNER JOIN customer On CUSTOMER.CUST_CODE = ORDERS.CUST_CODE ;
SELECT CUSTOMER.cust_name , CUSTOMER.cust_code , CUSTOMER.agent_code  FROM orders INNER JOIN customer On CUSTOMER.agent_CODE = ORDERS.agent_code;
SELECT CUSTINFO.ACCNO , CUSTINFO.CLIENT_NAME , CUSTINFO.MOBILE_NO , MAIN_TRANSACTION_CASH.BALANCE AS CASH_BALANCE FROM CUSTINFO INNER JOIN MAIN_TRANSACTION_CASH ON CUSTINFO.ACCNO = MAIN_TRANSACTION_CASH.ACCNO ;
SELECT CUSTINFO.ACCNO , CUSTINFO.CLIENT_NAME , CUSTINFO.MOBLIE_NO , CUSTINFO.VERIFICATION FROM CUSTINFO INNER JOIN MAIN_TRANSACTION_CASH ON CUSTINFO.ACCNO = MAIN_TRANSACTION_CASH.ACCNO ;
